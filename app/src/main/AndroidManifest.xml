<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.suiseiseki.www.suiseilauncher">

    <application android:allowBackup="true" android:label="@string/app_name"
        android:icon="@mipmap/ic_launcher" android:theme="@style/AppTheme">

        <activity android:name=".SuiseiLauncherActivity"
            android:label = "Suiseiseki Launcher">
            <intent-filter>
                <action android:name = "android.intent.action.MAIN"/>
                <category android:name = "android.intent.category.LAUNCHER"/>
                <category android:name = "android.intent.category_HOME"/>
                <category android:name = "android.intent_category_DEFAULT"/>
            </intent-filter>

        </activity>

    </application>

</manifest>
